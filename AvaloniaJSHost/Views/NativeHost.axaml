<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                          xmlns:local="using:AvaloniaJSHost.Views"
             x:Class="AvaloniaJSHost.Views.NativeHost">
  <StackPanel Grid.Column="0" Grid.Row="2" Orientation="Vertical">
    <Button x:Name="ClearCanvas" Content="Очистить"/>
    <local:DirectedControl x:Name="JSCanvas"/>
  </StackPanel>
</UserControl>
